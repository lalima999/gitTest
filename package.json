{
  "name": "automation-testing-selenium",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test-serial": "mocha test/**/*.test.js",
    "test-parallel": "mocha --parallel ./test/**/*.test.js",
    "test-mochawesome": "mocha test/**/*.test.js --reporter mochawesome --reporter-options reportDir=./TestResults/NormalExecution",
    "test-parallel-mochawesome": "mocha --parallel ./test/**/*.test.js --reporter mochawesome --reporter-options reportDir=./TestResults/ParallelExecution"
  },
